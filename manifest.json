{
    "manifest_version": 3,
    "name": "Social Media Blocker",
    "version": "1.0",
    "description": "A Chrome extension to close tabs with YouTube URLs",
    "permissions": ["tabs"],
    "action": {
      "default_icon": {
        "16": "assets/icon16.png",
        "32": "assets/icon32.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
      }
    },
    "icons": {
        "16": "assets/icon16.png",
        "32": "assets/icon32.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }